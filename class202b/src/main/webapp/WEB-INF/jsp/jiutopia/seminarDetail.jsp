					<td><%=seminarDetail.get("eventLoc")%></td>
				</tr>
				<tr>
					<td> 강사 </td>
					<td><%=seminarDetail.get("eventTeacher")%></td>
				</tr>					
				<tr>
					<td> 시작 날짜 </td>
					<td><%=seminarDetail.get("eventStart")%></td>
				</tr>	
				<tr>
					<td> 소개 </td>
					<td><%=seminarDetail.get("eventCont")%></td>
				</tr>	
			</table>
			</div>
		</div>
<div class = "gubun">
	<hr />
</div>
<!-- 댓글 입력 -->
