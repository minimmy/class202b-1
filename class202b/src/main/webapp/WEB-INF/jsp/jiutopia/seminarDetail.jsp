			<%
			if(id != null) {	
			%>
			<form id = commentAddFrm action ="<%=contextPath%>/jiutopia/seminarCmmtAdd.do" method ="post">
					<table class="table table-striped table-bordered table-hover">
						<tr>
							<td><input type="hidden" id="eventId" name="eventId" value="<%=eventId%>"></td>
							<td><input type="hidden" id="memId" name="memId" value="<%=id%>"></td>
							<td><input type ="text" id="cmmtCont" name ="cmmtCont" value=""></td>
							<td><input type="submit" value="입력"></td>
						</form>	
						<%
							}
						%>		
					</table>
			</div>
			<table class="table table-striped table-bordered table-hover">
					
				<tr>
					<th>번호</th>
					<th>내용</th>
					<th>이름</th>
					<th>날짜</th>
					<th></th>
				</tr>
				<%int i = listCnt; 
				for (EVCmmtVo row : eventCmmtList){ 
					String dateId = row.getCmmtId();
					String[] date = dateId.split(" ");
				%>
				<tr>
					<td> <%=i %></td>
					<td> <%=row.getCmmtCont()%></td>
					<td> <%=row.getMemId()%></td>
					<td> <%=date[0]%></td>
					<td>
						<%
							if(id != null && row.getMemId().equals(id)) {	
						%>
						<form action="<%=contextPath%>/jiutopia/seminarCmmtDel.do" method="post" name="cmmtDel" >
							<input type="submit" value="삭제">
							<input type="hidden" name="memId" value="<%=row.getMemId()%>">
							<input type="hidden" name="cmmtId" value="<%=row.getCmmtId()%>">
							<input type="hidden" name="eventId" value="<%=eventId%>">
						</form>
						<%
							}
						%>
						</td>
				<%i--;} %>		
			</tr>
			</table>
			
		</div>
	</div>		
</div>
